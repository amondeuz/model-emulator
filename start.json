{
  "daemon": true,
  "run": [
    {
      "method": "log",
      "params": {
        "raw": "Starting Puter Local Model Emulator..."
      }
    },
    {
      "method": "shell.run",
      "params": {
        "message": "node server/index.js",
        "path": "."
      },
      "on": [
        {
          "event": "/Puter Local Model Emulator started on (http:\/\/localhost:\d+)/",
          "set": {
            "baseUrl": "$1"
          },
          "done": true
        }
      ]
    },
    {
      "method": "local.set",
      "params": {
        "uri": "start.json",
        "data": {
          "uiUrl": "{{baseUrl}}/config.html"
        }
      }
    }
  ]
}
