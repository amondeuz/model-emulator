{
  "daemon": true,
  "run": [
    {
      "method": "log",
      "params": {
        "raw": "Starting Virtual Model Emulator..."
      }
    },
    {
      "method": "shell.run",
      "params": {
        "message": "python -m server.main",
        "on": [
          {
            "event": "/http:\\/\\/localhost:[0-9]+\\/config\\.html/",
            "done": true
          }
        ]
      }
    },
    {
      "method": "local.set",
      "params": {
        "url": "{{input.event[0]}}"
      }
    },
    {
      "method": "log",
      "params": {
        "raw": "UI ready at {{input.event[0]}}"
      }
    }
  ]
}
